#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

void Add(vector<vector<string>>& data, string s, int i){
	data[i-1].push_back(s);
}

void Next(vector<vector<string>>& data, int& currentMounth, const int nextDaysCount){
	 vector<vector<string>> delta;

	if (data.size() > nextDaysCount){
		for (auto i = data.size() - nextDaysCount - 1; i < data.size(); i++){
			delta.push_back(data[i]);
		}
		data.resize(nextDaysCount);
		printDeals(delta);
		for(auto& s : delta){
			data[nextDaysCount - 1].insert(end(data[nextDaysCount - 1]), begin(s), end(s));
		}
	}
	else {
		data.resize(nextDaysCount);
	}

	currentMounth != 11 ? currentMounth++ : 0;
}

void Dump(const vector<vector<string>>& data, const int& i){
	cout << data[i-1].size() << " ";
	for (auto& s : data[i-1]){
		cout << s << " ";
	}
	cout<<endl;
}

int main() {
	const vector<int> dayNumbers = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};

	int currentMounth = 0;
	vector<vector<string>> deals(dayNumbers[currentMounth]);

	int Q;
	cin >> Q;

	string command;
	string s;
	int x;
	for (auto i = 0; i < Q; i++){
		cin >> command;
		if (command == "ADD"){
			cin >> x;
			cin >> s;
			Add(deals, s, x);
		}
		if (command == "DUMP"){
			cin >> x;
			Dump(deals, x);
		}
		if (command == "NEXT"){
			Next(deals, currentMounth, dayNumbers[currentMounth + 1]);
		}

		s="";
		x=0;

//		cout << "Command" << i + 1 << ": ";
//		printDeals(deals);
	}

	return 0;
}
