#include <iostream>
#include <set>
#include <vector>

using namespace std;

int main() {
	const vector<string> known_commands = {"ADD", "COUNT", "CHECK"};
	vector<set<string>> synonyms;

	int Q;
	cin >> Q;
	string command;
	for (auto i = 0; i < Q; i++){
		cin >> command;
		if (command == known_commands[0]){
			string word1, word2;
			cin >> word1;
			cin >> word2;
			set<string>  tmp = {word1, word2};
			bool contains = false;
			for (auto& item : synonyms){
				if (item.count(word1) != 0 || item.count(word2) != 0){
					item.insert(begin(tmp), end(tmp));
					contains = true;
				}
			}

		} else if (command == known_commands[1]){
			string word;
			for (auto& item : synonyms){
				if (item.count(word) != 0) {
					cout << item.size() << endl;
					break;
				}
			}
		} else if(command == known_commands[2]) {
			string word1, word2;
			cin >> word1;
			cin >> word2;
			string result = "NO";
			for (auto& item : synonyms){
				if (item.count(word1) != 0 && item.count(word2) != 0)
				{
					result = "YES";
					break;
				}
			}
			cout << result <<endl;
		}
	}

	return 0;
}
