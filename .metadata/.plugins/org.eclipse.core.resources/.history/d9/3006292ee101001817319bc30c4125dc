#include <iostream>
#include <vector>

using namespace std;

int N, modulo;
bool needToStop = false;
std::vector<int> binary_number;

int main() {

	cin>>N;

	while (N > 2){
		modulo = N % 2;
		binary_number.push_back(modulo);
		N /= 2;
	}

	for (int i=0; i< binary_number.size; i++){

	}

	return 0;
}
