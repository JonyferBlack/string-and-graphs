#include <iostream>
#include <cmath>
using namespace std;

int main(){
    double a, b, c ,D;

    cin>>a>>b>>c;

    if (a == 0)
    {
      if (b != 0)
        std::cout << -c/b << endl;
      return 0;
    }

 //   if (a != 0 && b==0 && c==0) return 0;

    D = b*b-4*a*c;

    if(D>1){
    	cout<<(-b+sqrt(D))/(2*a)<<" ";
    	cout<<(-b-sqrt(D))/(2*a);
    	return 0;
    }
    if(D==0){
    	cout<<(-b+sqrt(D))/(2*a);
    	return 0;
    }

	return 0;
}
