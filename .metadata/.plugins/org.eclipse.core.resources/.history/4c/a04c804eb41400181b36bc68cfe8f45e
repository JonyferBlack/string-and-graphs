#include <iostream>
#include "string_exact_match.h"

using namespace std;
using namespace string_exact_match_exersises;

template<typename T>
void print_vector(const vector<T>& v){
	for(const auto& item : v){
		cout << item << " ";
	}
}

int main() {
//	cout << string_exact_match_naive("abcdefjdef", "de") << endl; // prints

	print_vector(z_algorithm_naive("aabcaabxaaz"));
	cout << endl;
	prit_vector(z_algorithm());
	return 0;
}
