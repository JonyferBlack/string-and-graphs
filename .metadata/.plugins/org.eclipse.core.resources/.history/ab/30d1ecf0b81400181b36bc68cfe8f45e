#include <iostream>
#include "string_exact_match.h"

using namespace std;
using namespace string_exact_match_exersises;

template<typename T>
void print_vector(const vector<T>& v){
	for(const auto& item : v){
		cout << item << " ";
	}
}

int main() {
	const string sample_text = "aabcaabxaazaabc";
	const string sample_pattern = "aab";

	cout << string_match_naive(sample_text, sample_pattern) == linear_time_pattern_search(sample_text, sample_pattern) << std::endl; // prints

	return 0;
}
