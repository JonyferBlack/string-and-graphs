#include <iostream>
#include <map>
#include <vector>

using namespace std;

void NEW_BUS(map<int, vector<string>>& stops_by_bus, int bus, int stops_count, string* args){
	for (auto i = 0; i < stops_count; i++)
		stops_by_bus[bus].push_back(args[i]);
}

