#include <iostream>
#include <vector>

using namespace std;

vector<int> Reverse(const vector<int>& v){
	int tmp = 0;
	for (auto i=0; i < v.size()/2; ++i){
		tmp = v[i];
		v[i] = v[v.size()- i - 1];
		v[v.size()- i - 1] = tmp;
	}
}

int main() {

	return 0;
}
