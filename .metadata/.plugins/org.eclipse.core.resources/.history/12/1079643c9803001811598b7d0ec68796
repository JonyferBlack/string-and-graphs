#include <vector>
#include <iostream>
using namespace std;

int Average(vector<int> sequence){
	int result=0;
	for(auto& temp : sequence){
    	result+=temp;
    }

}

int main() {
	int N;
	cin>>N;

	vector<int> day_temps(N);
	for(auto& temp : day_temps){
    	cin>>temp;
    }

	for(auto& temp : day_temps){
    	cin>>temp;
    }


	return 0;
}
