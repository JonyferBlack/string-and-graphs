#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int a,b,c,D;
    cin>>a>>b>>c;
    if (a == 0)
    {
      if (b != 0)
        std::cout << -1.*c/b << endl;
      else if (c == 0)
        std::cout << "Все коэффициенты равны нулю; x - любое число." << std::endl;
      else
        std::cout << "Нет решений." << std::endl;
    }
    D = b*b-4*a*c;
    if(D>1){
    	cout<<(-b+sqrt(D))/(2*a)<<" ";
    	cout<<(-b-sqrt(D))/(2*a);
    	return 0;
    }
    if(D==0){
    	cout<<(-b+sqrt(D))/(2*a);
    	return 0;
    }
	return 0;
}
