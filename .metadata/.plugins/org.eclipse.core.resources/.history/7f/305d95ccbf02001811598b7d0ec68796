#include <iostream>
#include <vector>

using namespace std;

vector<int> Reversed(const vector<int>& v){
	vector<int> result;
	auto size = v.size();

	for (auto i = 0; i < size; i++){
		result[size - i - 1] = v[i];
	}

	return result;
}


int main() {
	vector<int> numbers = {1, 5, 3, 4, 2};

	for (auto x : Reversed(numbers)){
		cout<<x<<" ";
	}

	return 0;
}
