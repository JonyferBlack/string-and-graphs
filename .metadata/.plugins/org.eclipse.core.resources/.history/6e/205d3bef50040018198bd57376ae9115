	const vector<int> dayNumbers = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
	int currentMounth = 0;
	vector<string> deals(dayNumbers[currentMounth]);

	int Q;
	cin >> Q;

	for (auto i = 0; i < Q; i++){

	}


	return 0;
}
