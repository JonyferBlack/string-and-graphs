#include <iostream>
#include <vector>
using namespace std;

int Worry_count(const vector<bool>& queue){
	int count = 0;
	for (const auto& person : queue){
		if (person) count++;
	}
	return count;
}

int main() {
	const vector<string> cmds = {"WORRY", "QUIET", "COME", "WORRY_COUNT"};

	int N;
	cin>>N;

	vector<string> commands(N);
	vector<int> arguments(N);
	for (auto i = 0; i < N; i++){
    	auto& temp = commands[i];
		cin >> temp;
    	if (temp == cmds[0] || temp == cmds[1] || temp == cmds[2])
    		cin >> arguments[i];
    }

	vector<bool> queue;

	for (auto i = 0; i < commands.size(); i++){
		auto& cmd = commands[i];
		auto& arg = volumes[i];

		if (cmd == cmds[0]){
			queue[] = true;
			i++;
			continue;
		}
		if (cmd == cmds[1]){
				queue[volumes[i]] = false;
				i++;
				continue;
		}

		if (cmd == cmds[2]){
				auto count = volumes[i];
				if (count > 0) {
					queue.assign(count, false);
				}
				else if (count < 0){
					queue.assign(-count, false);
				}
				i++;
				continue;
		}

		if (cmd == cmds[3]){
			cout<< Worry_count(queue) << endl;
		}
	}

	return 0;
}
